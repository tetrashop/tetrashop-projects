{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "service-interfaces/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "*.json",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dashboard-main.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["fra1"],
  "github": {
    "silent": true,
    "autoJobCancelation": true
  },
  "public": true
}
