{
  "version": 2,
  "builds": [
    {
      "src": "quantum-start-fixed.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "quantum-start-fixed.js"
    },
    {
      "src": "/(.*)",
      "dest": "quantum-start-fixed.js"
    }
  ],
  "env": {
    "QUANTUM_MODE": "maximum_efficiency",
    "SELF_HEALING": "true",
    "MAX_PERFORMANCE": "true", 
    "NODE_ENV": "production",
    "OPTIMIZATION_LEVEL": "quantum"
  },
  "regions": ["all"]
}
