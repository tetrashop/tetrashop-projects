{
  "version": 2,
  "name": "tetrashop-suite",
  "builds": [
    {
      "src": "apps/core/tetrashop100.js",
      "use": "@vercel/node"
    },
    {
      "src": "apps/core/simple-tetrashop.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/apps/core/tetrashop100.js"
    },
    {
      "src": "/(.*)",
      "dest": "/apps/core/tetrashop100.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
