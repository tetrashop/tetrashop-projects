{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/chess",
      "dest": "/chess/index.html"
    },
    {
      "src": "/writer",
      "dest": "/writer/index.html"
    },
    {
      "src": "/quantum",
      "dest": "/quantum/index.html"
    },
    {
      "src": "/secret-garden",
      "dest": "/secret-garden/index.html"
    },
    {
      "src": "/speech-recognition",
      "dest": "/speech-recognition/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
