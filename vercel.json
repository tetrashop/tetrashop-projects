{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "regions": ["fra1"],
  "functions": {
    "src/**/*.ts": {
      "maxDuration": 10
    }
  },
  "env": {
    "NODE_ENV": "production",
    "VITE_APP_VERSION": "2.0.0"
  },
  "github": {
    "enabled": true,
    "silent": false
  }
}
